pipeline {

 agent any

 tools { 
        jdk 'JAVA_HOME' 
        maven 'M2_HOME' 
    }

 stages {

 stage('GIT') {

           steps {

               git branch: 'main',

               url: ' https://github.com/saifGt/projetTimeshet.git'

          }

     }

 stage ('Compile Stage') {

 steps {

 sh 'mvn clean compile'

 }

 }
stage('MVN SONARQUBE') {
    steps {
        sh 'mvn sonar:sonar -Dsonar.token=sonar-token-12345 -Dmaven.test.skip=true'
    }
}

 }

}
