pipeline {

 agent any

 tools { 
        jdk 'JAVA_HOME' 
        maven 'M2_HOME' 
    }

 stages {

 stage('GIT') {

           steps {

               git branch: 'main',

               url: ' https://github.com/saifGt/projetTimeshet.git'

          }

     }

 stage ('Compile Stage') {

 steps {

 sh 'mvn clean compile'

 }

 }
stage('SonarQube Analysis') {
            steps {
                sh 'mvn sonar:scanner -Dsonar.login=admin -Dsonar.password=sonar -Dmaven.test.skip=true';
            }
 }
}
}
